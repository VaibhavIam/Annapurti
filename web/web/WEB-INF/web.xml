<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Controller.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>model.Main</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Feed_form</servlet-name>
        <servlet-class>Controller.Feed_formC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Donor_feed</servlet-name>
        <servlet-class>Donor_feed</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Feed_info</servlet-name>
        <servlet-class>Controller.Feed_info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ngo_info</servlet-name>
        <servlet-class>Controller.Ngo_info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Notification_accepted</servlet-name>
        <servlet-class>Controller.Notification_accepted</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Notification_rejected</servlet-name>
        <servlet-class>Controller.Notification_rejected</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Request_done</servlet-name>
        <servlet-class>Controller.Request_done</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/Main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Feed_form</servlet-name>
        <url-pattern>/Feed_form</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Donor_feed</servlet-name>
        <url-pattern>/Donor_feed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Feed_info</servlet-name>
        <url-pattern>/Feed_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ngo_info</servlet-name>
        <url-pattern>/Ngo_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Notification_accepted</servlet-name>
        <url-pattern>/Notification_accepted</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Notification_rejected</servlet-name>
        <url-pattern>/Notification_rejected</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Request_done</servlet-name>
        <url-pattern>/Request_done</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            -1
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
